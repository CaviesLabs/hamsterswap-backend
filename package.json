{
  "engines": {
    "node": "18",
    "npm": "8.8.0"
  },
  "name": "hamsterpassport",
  "version": "0.0.1",
  "description": "Hamsterpassport package.",
  "author": "khang@cavies.xyz",
  "private": true,
  "type": "commonjs",
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:run": "yarn typeorm migration:run -- -d ./src/orm/type-orm.config.ts",
    "migration:generate": " rm -rf dist && yarn typeorm -- -d ./src/orm/type-orm.config.ts migration:generate ./src/orm/migrations/Migration",
    "migration:create": "yarn typeorm -- migration:create ./src/orm/migrations/Migration",
    "migration:revert": "yarn typeorm -- -d ./src/orm/type-orm.config.ts migration:revert",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start-test-keycloak": "node ./scripts/start-keycloak-test-server.mjs",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "CONFIG_FILE=\"./e2e-test.config.json\" npx nyc --reporter=lcov --reporter=text --reporter=clover --reporter=text-summary mocha --timeout 25000 --require ts-node/register test/unit/test-entrypoint.spec.ts",
    "test:e2e": "CONFIG_FILE=\"./e2e-test.config.json\" npx nyc --reporter=lcov --reporter=text --reporter=clover --reporter=text-summary mocha --timeout 25000 --require ts-node/register test/e2e/test-entrypoint.e2e-spec.ts",
    "test:e2e-dev": "CONFIG_FILE=\"./e2e-test.config.json\" npx mocha --timeout 25000 --reporter=spec --full-trace --require ts-node/register test/e2e/test-entrypoint.e2e-spec.ts",
    "prepare": "husky install"
  },
